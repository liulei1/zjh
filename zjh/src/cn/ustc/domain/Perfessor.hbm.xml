<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.ustc.domain.Professor" table="professor" catalog="zjh">
		<id name="id">
			<generator class="uuid"/>
		</id>
		
		<property name="name"/>
		<property name="password" type="string"></property>
		<property name="email" type="string"></property>
		<property name="sex" type="string"></property>
		<property name="usertype" type="string"></property>
		<property name="real_name"/>
		<property name="image"/>
		<property name="telephone"/>
		<property name="address"/>
		<property name="authority"/>
		<property name="website"/>
		<property name="remark"/>
	</class>
</hibernate-mapping>