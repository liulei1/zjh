<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.ustc.domain.ConsultCheck" table="consult_check" catalog="zjh">
		<id name="id">
			<generator class="native"/>
		</id>
		
		<property name="cons_id"/>
		<property name="admin_id"/>
		<property name="check_datetime"/>
		<property name="state"/>
		<property name="rejectReason"/>
	</class>
</hibernate-mapping>